{
	"name": "dashdesigner",
	"version": "1.0.0",
	"description": "A theme designer compatible with the DashUI package.",
	"scripts": {
		"start": "webpack-dev-server --config webpack.dev.config.js --open",
		"build:dev": "webpack --config webpack.dev.config.js --progress",
		"build:prod": "webpack --config webpack.prod.config.js --progress",
		"test": "jest",
		"format": "npm run format:prettier && npm run format:tslint",
		"format:prettier": "prettier --write ./src",
		"format:tslint": "tslint -p tsconfig.json -c tslint.json ./src/**/*.{ts,tsx,js,jsx} --fix"
	},
	"lint-staged": {
		"./src/**/*.{ts,tsx,js,jsx}": [
			"tslint -p tsconfig.json -c tslint.json --fix",
			"prettier --write"
		],
		"./src/**/*.scss": [
			"prettier --write"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"keywords": [],
	"author": "Nick Barnette",
	"license": "UNLICENSED",
	"dependencies": {
		"@nickbarnette/dashui": "^0.7.5",
		"@types/react-router-dom": "^5.1.5",
		"classnames": "^2.2.6",
		"mini-css-extract-plugin": "^0.9.0",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-redux": "^7.2.2",
		"react-router": "^5.2.0",
		"react-router-dom": "^5.2.0",
		"redux": "^4.0.5",
		"redux-persist": "^6.0.0"
	},
	"devDependencies": {
		"@babel/core": "^7.10.2",
		"@babel/preset-env": "^7.10.2",
		"@babel/preset-react": "^7.10.1",
		"@babel/preset-typescript": "^7.10.1",
		"@storybook/addon-contexts": "^5.3.19",
		"@storybook/react": "^5.3.19",
		"@testing-library/react": "^10.2.1",
		"@types/chai": "^4.2.11",
		"@types/classnames": "^2.2.10",
		"@types/jest": "^26.0.0",
		"@types/mocha": "^7.0.2",
		"@types/react": "^16.9.36",
		"@types/react-dom": "^16.9.8",
		"@types/react-redux": "^7.1.11",
		"@types/react-router": "^5.1.7",
		"babel-loader": "^8.1.0",
		"chai": "^4.2.0",
		"copy-webpack-plugin": "^6.0.4",
		"core-js": "^3.6.5",
		"css-loader": "^3.6.0",
		"gh-pages": "^3.1.0",
		"husky": "^4.2.5",
		"jest": "^26.0.1",
		"lint-staged": "^10.2.13",
		"mocha": "^8.0.1",
		"node-sass": "^4.14.1",
		"prettier": "^2.0.5",
		"sass-loader": "^8.0.2",
		"style-loader": "^1.2.1",
		"ts-jest": "^26.1.0",
		"ts-loader": "^7.0.5",
		"ts-node": "^8.10.2",
		"tslint": "^6.1.2",
		"tslint-immutable": "^6.0.1",
		"typescript": "^3.9.5",
		"webpack": "^4.43.0",
		"webpack-cli": "^3.3.11",
		"webpack-dev-server": "^3.11.0"
	}
}
